<h2>Create new user</h2>
<div class="row">
    <div class="span6 offset3">
        <%= form_for(@user) do |f| %>
            <%= render 'shared/error_messages', object: f.object %>
        <% end %>
        <%= form_for @user do |f| %>
            <%= render partial: "shared/user_form", locals: {f: f} %>
            <p><%= f.submit "Create" %></p>
        <% end %>
    </div>
</div>